{
  "name": "stylica-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "client:dev": "vite",
    "client:dev:local": "vite --mode development",
    "client:dev:prod": "vite --mode production",
    "build": "tsc -b && vite build",
    "build:local": "tsc -b && vite build --mode development",
    "build:prod": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "server:dev": "nodemon",
    "server:dev:local": "cross-env NODE_ENV=development nodemon",
    "server:dev:prod": "cross-env NODE_ENV=production nodemon",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "dev:local": "concurrently \"npm run client:dev:local\" \"npm run server:dev:local\"",
    "dev:prod": "concurrently \"npm run client:dev:prod\" \"npm run server:dev:prod\"",
    "dev:api": "tsx watch api/server.ts",
    "build:api": "tsc --project api/tsconfig.json",
    "start:api": "node dist/api/server.js",
    "dev:full": "concurrently \"npm run dev:api\" \"npm run dev\"",
    "tunnel": "node ngrok-setup.js",
    "env:local": "node setup-env.cjs local",
    "env:production": "node setup-env.cjs production",
    "env:help": "node setup-env.cjs --help"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.54.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "framer-motion": "^12.23.12",
    "helmet": "^8.0.0",
    "i18next": "^25.3.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "ngrok": "^5.0.0-beta.2",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^7.3.0",
    "recharts": "^3.1.2",
    "sonner": "^1.7.0",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.0.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.15.30",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
